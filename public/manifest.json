{
  "manifest_version": 3,
  "name": "MyOneTab",
  "version": "1.0",
  "description": "A simple Chrome extension.",
  "permissions": ["storage", "tabs"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [
    {
      "resources": ["page.html"],
      "matches": ["<all_urls>"]
    }
  ]
}
